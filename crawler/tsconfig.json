{
	"extends": "../tsconfig.base.json",
	"compilerOptions": {
		"outDir": "out",
		"rootDir": "src",
		"module": "ES2022",
		"target": "ES2022",
		"moduleResolution": "Bundler",
		"lib": [
			"ES2022"
		],
		"types": [
			"node"
		],
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"strict": true,
		"noEmit": false,
		"noEmitOnError": false,
		"allowImportingTsExtensions": false
	},
	"include": [
		"src/**/*"
	]
}
